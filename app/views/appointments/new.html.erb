<div class="container">

  <%= simple_form_for @appointment do |f| %>
    <%= f.error_notification %>
    <!-- en caché nom prénom téléphone du current user-->
    <%= f.input :date, as: :date, html5: true, label: 'date ici' %>
    <%= f.input :time, as: :time, html5: true, label: 'heure ici' %>
    <%#= f.input :user_ids, as: :hidden %>
    <%= f.button :submit, 'confirmer', class:"button" %>
  <% end %>

<div id="calendar">  </div>

</div>
